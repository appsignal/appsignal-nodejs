{
  "name": "@appsignal/nextjs",
  "version": "1.0.9",
  "main": "dist/index",
  "types": "dist/index",
  "license": "MIT",
  "dependencies": {
    "@appsignal/types": "^2.0.2",
    "tslib": "^2.0.3"
  },
  "peerDependencies": {
    "next": ">= 9.3.0"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:ext": "node-gyp rebuild",
    "build:watch": "tsc -p tsconfig.json -w --preserveWatchOutput",
    "clean": "rimraf dist coverage",
    "link:npm": "npm link",
    "link:yarn": "yarn link",
    "test": "jest --passWithNoTests && BUNDLE_GEMFILE=test/Gemfile bundle install --quiet && BUNDLE_GEMFILE=test/Gemfile bundle exec rspec test",
    "test:watch": "jest --watch"
  },
  "publishConfig": {
    "access": "public"
  }
}
